{
  "editor.semanticHighlighting.enabled": true,
  "editor.mouseWheelZoom": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "material-icon-theme",
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "custom",
  "editor.padding.top": 6,
  "window.menuStyle": "custom",
  "window.zoomLevel": -0.1,
  "workbench.editor.labelFormat": "short",
  "terminal.integrated.hideOnStartup": "always",
  "terminal.integrated.gpuAcceleration": "on",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.scrollbar.horizontalScrollbarSize": 8,
  "editor.smoothScrolling": true,
  // "workbench.editor.showTabs": "none",
  "editor.fontSize": 15,
  "extensions.ignoreRecommendations": true,
  "redhat.telemetry.enabled": false,
  "editor.suggest.preview": true,
  // "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.enabled": false,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "extensions.autoUpdate": false,
  "workbench.statusBar.visible": true,
  "telemetry.telemetryLevel": "off",
  "workbench.activityBar.location": "top",
  "workbench.sideBar.location": "left",
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": "explicit",
      "source.organizeImports.ruff": "explicit",
      "source.fixAll.markdownlint": "explicit"
    }
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true,
  },
  "files.autoSave": "afterDelay",
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnSave": true,
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnSave": true,
  },
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint",
    "editor.formatOnSave": true,
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true,
  },
  "editor.formatOnSave": true,
  "gopls": {
    "ui.semanticTokens": true
  },
  "material-icon-theme.hidesExplorerArrows": true,
  "editor.formatOnPaste": true,
  "todo-tree.highlights.enabled": false,
  "todo-tree.filtering.ignoreGitSubmodules": true,
  "explorer.confirmDelete": false,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.autoIndent": "advanced"
  },
  "[yml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.autoIndent": "advanced"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.autoIndent": "advanced"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.autoIndent": "advanced"
  },
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontLigatures.enabled": true,
  "workbench.startupEditor": "none",
  "explorer.confirmDragAndDrop": false,
  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false
  },
  "svelte.enable-ts-plugin": true,
  "C_Cpp.intelliSenseEngine": "disabled",
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "explorer.confirmPasteNative": false,
  "latex-workshop.formatting.latex": "tex-fmt",
  "[latex]": {
    "editor.formatOnPaste": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "latex-workshop.bibtex-format.sort.enabled": true,
  "latex-workshop.latex.autoClean.run": "onSucceeded",
  "[postcss]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "debugpy.showPythonInlineValues": true,
  "debug.terminal.clearBeforeReusing": true,
  "ruff.nativeServer": true,
  "python.languageServer": "Jedi",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.venvPath": "${workspaceFolder}/.venv",
  "syncing.separateKeybindings": false,
  "continue.telemetryEnabled": false,
  "yaml.schemas": {
    "file:///home/leonne/.vscode-oss/extensions/continue.continue-1.2.4-linux-x64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  },
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "trailing-spaces.trimOnSave": true,
  "periscope.showPreviousResultsWhenNoMatches": true,
  "nixEnvSelector.useFlakes": true,
  "find-it-faster.advanced.disableStartupChecks": true,
  "find-it-faster.general.openFileInPreviewEditor": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "files.autoSaveWorkspaceFilesOnly": true,
  "files.autoSaveWhenNoErrors": true,
  "files.autoSaveDelay": 500,
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.middleClickBehavior": "paste",
  "terminal.integrated.rightClickBehavior": "paste",
  "terminal.integrated.shellIntegration.history": 1000,
  "terminal.integrated.suggest.cdPath": "relative",
  "gitlens.terminal.overrideGitEditor": false,
  "workbench.editor.revealIfOpen": true,
  "search.quickOpen.includeSymbols": true,
  "workbench.editorAssociations": {
    "{hexdiff}:/**/*.*": "hexEditor.hexedit",
    "*.svg": "default"
  },
  "markdownlint.config": {
    "MD033": false,
  }
}